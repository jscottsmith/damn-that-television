{
  "name": "damn-that-television",
  "version": "0.0.1",
  "engines": {
    "node": ">=5.0.0"
  },
  "private": false,
  "description": "Work by J Scott Smith",
  "scripts": {
    "start": "npm run prod",
    "postinstall": "npm run build",
    "dev": "NODE_ENV=development NODE_PATH=./src node ./src/server/server.babel.js",
    "prod": "NODE_ENV=production NODE_PATH=./src node ./src/server/server.babel.js",
    "build": "npm run build:client && npm run build:server",
    "build:server": "NODE_ENV=production webpack --config ./webpack/server.babel.js --display-modules",
    "build:client": "NODE_ENV=production webpack --config ./webpack/client.babel.js --display-modules",
    "build:dev": "npm run build:dev:client",
    "build:dev:client": "NODE_ENV=development webpack --config ./webpack/dev.client.babel.js --display-modules",
    "build:dev:server": "NODE_ENV=development webpack --config ./webpack/dev.server.babel.js --display-modules",
    "lint": "eslint -c .eslintrc src --ext .js,.jsx",
    "lint-autofix": "eslint -c .eslintrc --fix src --ext .js,.jsx",
    "db:migrate": "./node_modules/.bin/sequelize --config ./src/database/config/config.js db:migrate",
    "db:seed:all": "./node_modules/.bin/sequelize --config ./src/database/config/config.js db:seed:all"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/jscottsmith/damn-that-television.git"
  },
  "devDependencies": {
    "assets-webpack-plugin": "^3.5.1",
    "autoprefixer": "^7.1.1",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-register": "^6.24.0",
    "css-loader": "^0.28.4",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.2",
    "html-loader": "^0.5.1",
    "markdown-loader": "^2.0.1",
    "node-sass": "^4.5.3",
    "postcss-loader": "^2.0.6",
    "raw-loader": "^0.5.1",
    "react-hot-loader": "^3.0.0-beta.6",
    "sass-loader": "^6.0.6",
    "sequelize-cli": "^2.8.0",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "webpack": "^3.0.0",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-hot-middleware": "^2.18.0",
    "webpack-merge": "^4.1.0",
    "webpack-node-externals": "^1.6.0"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "body-parser": "^1.17.2",
    "classnames": "^2.2.5",
    "colors": "^1.1.2",
    "compression": "^1.7.0",
    "connect-session-sequelize": "^4.1.0",
    "dotenv": "^4.0.0",
    "express": "^4.15.2",
    "express-session": "^1.15.3",
    "gsap": "^1.20.2",
    "helmet": "^3.6.1",
    "history": "^4.6.1",
    "immutable": "^3.8.1",
    "lodash": "^4.17.4",
    "pg": "^6.4.0",
    "pg-hstore": "^2.3.2",
    "react": "^15.4.2",
    "react-addons-transition-group": "^15.6.0",
    "react-dom": "^15.4.2",
    "react-helmet": "^5.1.3",
    "react-redux": "^5.0.3",
    "react-router": "^4.0.0",
    "react-router-config": "^1.0.0-beta.3",
    "react-router-dom": "^4.0.0",
    "react-router-redux": "^5.0.0-alpha.4",
    "react-transition-group": "^2.2.0",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "sequelize": "^4.0.0"
  }
}
