          }
        },
        "start_date": {
          "type": "Date",
          "config": {
            "label": "Start Date"
          }
        },
        "end_date": {
          "type": "Date",
          "config": {
            "label": "End Date"
          }
        },
        "company_website": {
          "type": "Link",
          "config": {
            "label": "Company Website",
            "select": null
          }
        },
        "keywords": {
          "type": "Text",
          "config": {
            "label": "Keywords"
          }
        }
      }
    }
  ]
